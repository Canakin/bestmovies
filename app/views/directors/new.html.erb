<%= simple_form_for [@movie, @director], url: movie_directors_path do |f| %>
  <%= f.input :fullname %>
  <%= f.input :birthyear %>
  <%= f.input :minibio %>
  <%= f.input :nationality %>
  <%= f.input :top_movies %>
  <%= f.input :photo, as: :file %>
  <%= f.button :submit, class:'btn btn-primary' %>
<% end %>
