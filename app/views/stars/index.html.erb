<div id='stars'>
  <ul>
  <% @stars.each do |star| %>
    <li><%="#{star.fullname}"%>
    <% if star.photo.attached?  %>
      <%= image_tag star.photo, class:'img_top' %>
    <% end %>
    </li>
  <% end %>
  </ul>
</div>

<div class='btn btn primary'>
  <button><%= link_to 'New Actor', new_movie_star_path %></button>
</div>
